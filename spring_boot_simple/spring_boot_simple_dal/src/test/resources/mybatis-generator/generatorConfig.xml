<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <context id="mysqlGenerator" targetRuntime="MyBatis3">
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true" />
            <property name="suppressDate" value="false"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/springmybatis?useUnicode=true&amp;characterEncoding=UTF-8"
                        userId="root" password="Qq123456" />
        <javaModelGenerator targetPackage="com.kang.boot.dal.model" targetProject="src\main\java" />
        <sqlMapGenerator targetPackage="mybatis" targetProject="src\main\resources" />
        <javaClientGenerator targetPackage="com.kang.boot.dal.mapper" targetProject="src\main\java" type="XMLMAPPER" />

        <table tableName="sys_roles" domainObjectName="SysRoles" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
        </table>

        <table tableName="sys_users" domainObjectName="SysUsers" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
        </table>

        <table tableName="usr_role_link" domainObjectName="UsrRoleLink" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
        </table>
    </context>
</generatorConfiguration>